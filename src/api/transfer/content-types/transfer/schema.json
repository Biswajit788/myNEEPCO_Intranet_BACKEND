{
  "kind": "collectionType",
  "collectionName": "transfers",
  "info": {
    "singularName": "transfer",
    "pluralName": "transfers",
    "displayName": "Transfer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "OrderNo": {
      "type": "string",
      "required": true,
      "regex": "^\\d+$"
    },
    "OrderDt": {
      "required": true,
      "type": "date"
    },
    "File": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "files"
      ]
    },
    "Description": {
      "type": "text",
      "required": true,
      "maxLength": 100,
      "default": "Click on the link below to download transfer and posting order."
    }
  }
}
